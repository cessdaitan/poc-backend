    "helmet": "^4.1.1",
    "husky": "^4.3.0",
    "joi": "^17.2.1",
    "joiful": "^2.0.1",
    "memjs": "^1.2.2",
    "moment": "^2.29.1",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.21.10",
    "uuid": "^8.3.0"
  },
