{
  "name": "pathfinder-calculator",
  "version": "1.0.0",
  "description": "Pathfinder calculator",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "build": "tsc -pretty -p .",
    "start-offline": "npm run build && serverless offline start",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "jest --forceExit --detectOpenHandles",
    "test:lint": "eslint \"./src/**/*.ts\"",
    "deploy": "npm run build && sls deploy --force --verbose",
    "db:migrate": "npx sequelize db:migrate --env=database",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:lint",
      "pre-push": "npm run test:unit"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eucglobaldemo/pathfinder-calculator.git"
  },
  "author": "VMware",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eucglobaldemo/pathfinder-calculator/issues"
  },
  "homepage": "https://github.com/eucglobaldemo/pathfinder-calculator#readme",
  "dependencies": {
    "@azure/arm-commerce": "^2.1.1",
    "@azure/ms-rest-js": "^1.8.15",
